env:
  node: true
rules:
  # TODO: enable one by one (disabled for now to get eslint into CI)
  no-process-exit: 0
  no-undef: 0
  no-use-before-define: 0
  strict: 0
  no-unused-vars: 0
  no-shadow: 0
  handle-callback-err: 0
  consistent-return: 0
  no-redeclare: 0
  no-unreachable: 0
  no-unused-expressions: 0
  semi: 0
  eqeqeq: 0
  no-mixed-requires: 0
  comma-spacing: 0
  space-infix-ops: 0
  no-spaced-func: 0
  no-multi-spaces: 0
  key-spacing: 0
  dot-notation: 0
  curly: 0
  yoda: 0
  new-cap: 0
  camelcase: 0
  no-underscore-dangle: 0
  quotes: 0
  eol-last: 0
