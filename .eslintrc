env:
  node: true
rules:
  no-process-exit: 1
  no-undef: 1
  no-use-before-define: 1
  strict: 1
  no-unused-vars: 1
  no-shadow: 1
  handle-callback-err: 1
  consistent-return: 1
  no-redeclare: 1
  no-unreachable: 1
  no-unused-expressions: 1
  semi: 1
  eqeqeq: 1
  no-mixed-requires: 1
  comma-spacing: 1
  space-infix-ops: 1
  no-spaced-func: 1
  no-multi-spaces: 1
  key-spacing: 1
  dot-notation: 1
  curly: 1
  yoda: 1
  new-cap: 1
  camelcase: 1
  no-underscore-dangle: 0
  quotes: 1
  eol-last: 1
